# 生产环境配置
NODE_ENV=production

# API配置
VITE_API_BASE_URL=https://api.sust.edu.cn/api
VITE_API_MOCK=false

# 调试配置
VITE_APP_DEBUG=false
VITE_ENABLE_LOGGER=false

# 缓存配置
VITE_CACHE_ENABLED=true
VITE_CACHE_TTL=3600 # 生产环境缓存1小时

# 性能配置
VITE_ENABLE_COMPRESSION=true
VITE_COMPRESSION_THRESHOLD=1024 # 超过1KB的资源进行压缩
